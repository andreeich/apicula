{
  "name": "parcel-template",
  "version": "1.0.2",
  "license": "MIT",
  "scripts": {
    "dev": "bun clean && bun parcel ./src/*.html",
    "build": "bun clean && bun parcel build ./src/*.html",
    "production": "bun clean && bun parcel build ./src/*.html --no-optimize --no-source-maps --detailed-report",
    "clean": "rm -rf dist && rm -rf .parcel-cache",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@gatsbyjs/parcel-namer-relative-to-cwd": "^2.11.0",
    "@iconify-json/heroicons": "^1.1.11",
    "@iconify/tailwind": "^0.1.3",
    "@parcel/compressor-brotli": "^2.9.3",
    "@parcel/compressor-gzip": "^2.9.3",
    "@parcel/optimizer-cssnano": "^2.9.3",
    "@parcel/transformer-css": "^2.12.0",
    "@parcel/transformer-sass": "2.9.3",
    "@types/bun": "latest",
    "cssnano": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "parcel": "^2.9.3",
    "parcel-namer-custom": "^0.2.0",
    "path-browserify": "^1.0.0",
    "postcss": "^8.4.38",
    "postcss-modules": "^4.3.1",
    "posthtml-minify-classnames": "^0.4.0",
    "prettier": "3.2.5",
    "process": "^0.11.10",
    "sharp": "^0.31.3"
  },
  "dependencies": {
    "@splidejs/splide": "^4.1.4",
    "animejs": "^3.2.1",
    "aos": "^3.0.0-beta.6",
    "breakpoint-helper": "^2.0.4",
    "embla-carousel": "^8.0.4",
    "embla-carousel-autoplay": "^8.1.3",
    "scrollmagic": "^2.0.8",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "throttle-debounce": "^5.0.0"
  },
  "@parcel/runtime-js": {
    "splitManifestThreshold": 10000
  },
  "@parcel/transformer-css": {
    "cssModules": {
      "pattern": "my-company-[name]-[hash]-[local]"
    }
  },
  "parcel-namer-custom": {
    "favicon.": "[name].[type]",
    ".s?css": "css/[name].[type]",
    ".jsx?$": "js/[name].[type]",
    ".png$|.jpe?g$|.svg$": "assets/images/[name].[type]",
    ".mp4$": "assets/videos/[name].[type]",
    ".ttf$": "fonts/[folder]/[name].[type]",
    ".otf$": "fonts/[folder]/[name].[type]",
    ".woff$": "fonts/[folder]/[name].[type]",
    ".woff2": "fonts/[folder]/[name].[type]"
  }
}
